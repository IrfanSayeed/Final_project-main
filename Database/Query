SELECT Count(*) fROM age_2020
WHERE mental_indicator = 'Symptoms of Depressive or Anxiety Disorder';

SELECT Count(*) fROM age_2020
WHERE mental_indicator = 'Symptoms of Anxiety Disorder';

SELECT Count(*) fROM age_2020
WHERE mental_indicator = 'Symptoms of Depressive Disorder';

